    "version": "0.2.0",
    "configurations": [
        {
            "name": "Go: Current File",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "env": {},
            // "args": [
            //     "2",
            // ],
            "showLog": false
        },
        {
          "name": "Python: Current File",
          "type": "python",
          "request": "launch",
          "program": "${file}",
          "console": "integratedTerminal",
          "cwd": "${fileDirname}",
          "pythonArgs": [
            "-Xfrozen_modules=off"
          ],
          "env": {
            "PYDEVD_DISABLE_FILE_VALIDATION": "1"
          }
          // "args": ["-i", "response.xml", "-o", "response.csv"],
          // "args": ["-d", "/Volumes/Data"],
          // "args": ["-f", "menubar.dmg"],
          // "args": ["-h"],
        }
    ]
}
